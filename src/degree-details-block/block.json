{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "asu-degree-search/degree-details-block",
  "version": "2.0.0",
  "title": "ASU Degree Details",
  "category": "uds",
  "keywords": ["ASU", "details", "block", "degree"],
  "icon": "list-view",
  "description": "Provide a degree detail block, that will display all information about a specific degree.",
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "asu-degree-search-details-block",
  "viewScript": ["file:./frontend.js", "asu-degree-detail-view-script"],
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "attributes": {
		"anchorMenuEnabled":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuAtAGlance":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuApplicationRequirements":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuChangeMajorRequirements":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuNextSteps":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuAffordingCollege":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuFlexibleDegreeOptions":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuCareerOutlook":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuExampleCareers":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuGlobalOpportunity":{
			"type": "boolean",
			"default": false
		},
		"anchorMenuProgramContactInfo":{
			"type": "boolean",
			"default": false
		},
    "dataSourceAcadPlan":{
		"type": "string",
		"default": "BABUSGLBA"
	},
	"heroHide":{
		"type": "boolean",
		"default": false
	},
	"heroImageUrl":{
		"type": "string",
		"default": ""
	},
	"heroAltText":{
		"type": "string",
		"default": ""
	},
	"heroImageSize":{
		"type": "string",
		"enum": [ "small", "medium", "large" ],
		"default": "medium"
	},
	"introContentImageUrl":{
		"type": "string",
		"default": ""
	},
	"introContentImageAltText":{
		"type": "string",
		"default": ""
	},
	"introContentHideMarketText":{
		"type": "boolean",
		"default": false
	},
	"introContentHideProgramDesc":{
		"type": "boolean",
		"default": false
	},
	"introContentHideRequiredCourses":{
		"type": "boolean",
		"default": false
	},
	"atAGlanceHide":{
		"type": "boolean",
		"default": false
	},
	"atAGlanceApplicationRequirementsHide":{
		"type": "boolean",
		"default": false
	},
	"atAGlanceChangeMajorRequirementsHide":{
		"type": "boolean",
		"default": false
	},
	"affordingCollegeHide":{
		"type": "boolean",
		"default": false
	},
	"flexibleDegreeOptionsHide":{
		"type": "boolean",
		"default": false
	},
	"careerOutlookImageUrl":{
		"type": "string",
		"default": ""
	},
	"careerOutlookImageAltText":{
		"type": "string",
		"default": ""
	},
	"exampleCareersHide":{
		"type": "boolean",
		"default": false
	},
	"globalOpportunityImageUrl":{
		"type": "string",
		"default": ""
	},
	"globalOpportunityAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsFacultyImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsFacultyAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsProgramsImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsProgramsAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsResearchImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsResearchAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsInclusionImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsInclusionAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsMentorsImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsMentorsAltText":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsHonorsImageUrl":{
		"type": "string",
		"default": ""
	},
	"whyChooseAsuCardsHonorsAltText":{
		"type": "string",
		"default": ""
	},
	"attendOnlineImageUrl":{
		"type": "string",
		"default": ""
	},
	"attendOnlineAltText":{
		"type": "string",
		"default": ""
	}
  }
}
